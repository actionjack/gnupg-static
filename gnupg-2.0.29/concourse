    %echo Generating a basic OpenPGP key
    Key-Type: RSA
    Key-Length: 2049
    Subkey-Type: ELG-E
    Subkey-Length: 1024
    Name-Real: Concourse
    Name-Comment: Joe Concourse 
    Name-Email: joe@foo.bar
    Expire-Date: 0
    Passphrase: abc
    %pubring concourse.pub
    %secring concourse.sec
    # Do a commit here, so that we can later print "done" :-)
    %commit
    %echo done
